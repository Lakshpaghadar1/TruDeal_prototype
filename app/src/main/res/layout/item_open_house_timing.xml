<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textViewTiming"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10:30 AM - 11:30 AM"
        android:paddingVertical="@dimen/_10sdp"
        android:layout_marginVertical="@dimen/_2sdp"
        android:paddingHorizontal="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_7sdp"
        android:elevation="@dimen/_1sdp"
        android:textColor="@color/time_slot_text_color_selector"
        android:background="@drawable/selector_bg_white_grey_black_outlined"
        style="@style/SemiBold.10ssp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
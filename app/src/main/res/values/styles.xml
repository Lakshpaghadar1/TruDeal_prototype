<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>

        <item name="android:statusBarColor">@color/white</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="FullScreenTheme" parent="AppTheme">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>

    <style name="SplashTheme" parent="AppTheme">
        <item name="android:windowBackground">@drawable/layer_list_splash</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="TransparentProgressDialog">
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="BottomSheetStyle" parent="ThemeOverlay.MaterialComponents.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowSoftInputMode">adjustResize</item>
    </style>

    <style name="DialogStyle" parent="Base.Theme.AppCompat.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowMinWidthMajor">90%</item>
        <item name="android:windowMinWidthMinor">90%</item>
    </style>

    <style name="EditTextStyle" parent="Widget.AppCompat.EditText">
        <item name="android:paddingTop">@dimen/dp_0</item>
        <item name="android:paddingBottom">@dimen/dp_0</item>
        <item name="android:paddingStart">@dimen/_10sdp</item>
        <item name="android:paddingEnd">@dimen/_10sdp</item>
        <item name="android:drawablePadding">@dimen/_7sdp</item>

        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="fontFamily">@font/circular_std_book</item>
        <item name="android:fontFamily">@font/circular_std_book</item>
        <item name="android:textColor">@color/navyBlue</item>
        <item name="android:textColorHint">@color/gray</item>
        <item name="android:textAlignment">textStart</item>
    </style>

    <style name="RecyclerViewStyle">
        <item name="android:clipToPadding">false</item>
        <item name="android:overScrollMode">never</item>
    </style>

    <style name="NestedScrollViewStyle">
        <item name="android:fillViewport">true</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:overScrollMode">never</item>
    </style>

